    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "preLaunchTask": "swift-build",
            "name": "Debug",
            "program": "${workspaceFolder}/.build/debug/BERT-Russian",
            "args": [],
            "sourceLanguages": [
                "swift"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "preLaunchTask": "swift-build",
            "name": "Run lenet mnist",
            "program": "${workspaceFolder}/.build/debug/LeNet-MNIST",
            "args": [
                "-a",
                "4",
                "-n",
                "10",
                "--teacher-device",
                "eager",
                "--student-device",
                "eager"
            ],
            "sourceLanguages": [
                "swift"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "-1"
            }
        }
    ]
}